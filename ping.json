{
    "name": "ping",
    "location": "http://0.0.0.0",
    "port": 5001,
    "version": "1.0.6",
    "description": "Ping AI for test purposes only",
    "origin": "https://github.com/incubiq/osais_ai_ping",
    "default_cost": 1,
    "params": [{
        "in": "width",
        "out": "-width",
        "isMandatory": true,
        "type": "int",
        "default": 512,
        "accept": {"$in":[320, 384, 448, 512, 576, 640, 704, 768]},
        "freeAccess": {"$max": 512},
        "ui": {
            "title": "Output resolution",
            "widget": "uiMultiToggle"
        }
    }, {
        "in": "height",
        "out": "-height",
        "isMandatory": true,
        "type": "int",
        "default": 512,
        "accept": {"$in":[320, 384, 448, 512, 576, 640, 704, 768]},
        "freeAccess": {"$max": 512}
    }, {
        "in": "url_upload",
        "out": "url_upload",
        "isMandatory": true,
        "type": "string",
        "default": null,
        "ui": {
            "title": "Image input",
            "widget": "uiSelectPicture"
        }
    }, {
        "in": "output",
        "out": "-o",
        "isMandatory": true,
        "type": "function",
        "default": "$output_uid.ext"
    }, {
        "in": "idir",
        "out": "-idir",
        "isMandatory": false,
        "type": "string",
        "default": "./_input/"
    }, {
        "in": "odir",
        "out": "-odir",
        "isMandatory": false,
        "type": "string",
        "default": "./_output/"
    }]
}
