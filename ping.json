{
    "engine": "ping",
    "name": "Ping",
    "location": "",
    "ip": "0.0.0.0",
    "port": 5001,
    "version": "1.3.8",
    "description": "For Test and Integration purposes",
    "github": "https://github.com/incubiq/osais_ai_ping",
    "requiresGPU": false,
    "isFree": true,
    "default_cost": 10,
    "params": [{
        "in": "url_upload",
        "out": "-filename",
        "isMandatory": true,
        "type": "string",
        "value": {
            "default": null
        },
        "ui": {
            "title": "Image input",
            "widget": "uiSelectPicture",
            "filterExt": [".png", ".jpg", ".jpeg"],
            "maxWidth": 1024,
            "maxHeight": 1024,
            "minWidth": 256,
            "minHeight": 256,
            "ratio": -1,
            "maxKb": 1024,
            "column": 1
        }
    }, {
        "in": "output",
        "out": "-o",
        "isMandatory": true,
        "type": "function",
        "value": {
            "default": "$output_uid.ext"
        }
    }, {
        "in": "watermark",
        "out": "-watermark",
        "isMandatory": false,
        "type": "string",
        "value": {
            "default": null
        }
    }, {
        "in": "idir",
        "out": "-idir",
        "isMandatory": false,
        "type": "string",
        "value": {
            "default": "./_input/"
        }
    }, {
        "in": "odir",
        "out": "-odir",
        "isMandatory": false,
        "type": "string",
        "value": {
            "default": "./_output/"
        }
    }]
}
